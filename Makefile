git-jhonatas:
	@git config --global user.name "Murielson"
	@git config --global user.email "jhonsnoow32@gmail.com"
	@echo "\033[01;40mGIT CONFIGURADO COM SUCESSO!"
	@git config --global user.name
	@git config --global user.email
